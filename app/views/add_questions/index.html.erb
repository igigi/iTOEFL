<h1>Listing add_questions</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Mark</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_questions.each do |add_question| %>
      <tr>
        <td><%= add_question.content %></td>
        <td><%= add_question.mark %></td>
        <td><%= add_question.user %></td>
        <td><%= link_to 'Show', add_question %></td>
        <td><%= link_to 'Edit', edit_add_question_path(add_question) %></td>
        <td><%= link_to 'Destroy', add_question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Add question', new_add_question_path %>
